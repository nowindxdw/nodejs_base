{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["app.js", "models/", "schemas/", "swagger/", "test/"],
    "exclude": ["node_modules/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "encoding": "utf8",
  "recursive": true,
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    },
    "logoFile": "",
    "dateFormat": "YYYY-MM-DD",
    "outputSourceFiles": false,
    "outputSourcePath": true,
    "systemName": "后端文档",
    "footer": "dawei©️2016",
    "copyright": "dawei © 2015-2017",
    "navType": "vertical",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "html://",
    "methodHeadingReturns": false
  }
}